
pipeline{
agent any
stages{
	stage('Git Checkout') {
		steps{
		git branch 'main', url 'httpsgithub.comfrusteeespring-petclinic.git'
		}
	}
	stage('Set Maven Env') {
		steps{
		env.PATH = Capache-maven-3.8.5bin;cWindowsSystem32
		}
	}
	stage('Clean Test Package') {
		steps{
		bat 'mvn clean package'
		}
	}
	stage('Archive Jar') {
		steps{
		archive 'target.jar'
		}
	}

	}
	}