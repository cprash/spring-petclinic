
pipeline{
agent any
stages{
	stage('Git Checkout') {
		steps{
		echo 'Checking out!!!'
		git 'https://github.com/frusteee/spring-petclinic.git'
		}
	}
	stage('Set Maven Env') {
		steps{
		echo 'Setiing up Maven'
		script{
		env.PATH = 'C:/apache-maven-3.8.5/bin;c:\\Windows\\System32'
		}
		}
	}
	stage('Clean Package') {
		steps{
		echo 'Packaging Jar!!!'
		bat 'mvn clean package'
		}
	}
	stage('Archive Jar') {
		steps{
		echo 'Archiving Jar'
		archive 'target.jar'
		}
	}

	}
	}